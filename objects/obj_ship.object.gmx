            <string>var asteroids, forces_x, forces_y;

asteroids = 0;

var G = 100000;

with(obj_asteroid){
    var rel_x = x - other.x;
    var rel_y = y - other.y;
    
    var displacement = sqrt(rel_x * rel_x + rel_y * rel_y);
    var disp_angle = arctan2(rel_x, rel_y);
